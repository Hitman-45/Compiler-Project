#include <stdio.h>
int main(void)
{
	int a;
	int b;
	int c=5;
	char d;
	for(a=0;a<10;a=a+1)
	{
		b=a+1;
	}
	c=a+b;
    while(a==4 || b<7){
        while(c>0 && c<10) {
            c=c-1;
        }
        if(c==0) {
            a=1;
        }
        else {
            b=2;
            if(b==2) {
                b=3;
            }
        }
    }
    c=a-b;
}
